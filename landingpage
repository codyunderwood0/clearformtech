import React, { useMemo, useState } from "react";
import { motion } from "framer-motion";
import {
  ArrowRight,
  Bot,
  Shield,
  Workflow,
  Database,
  Smartphone,
  Lock,
  CheckCircle2,
  Sparkles,
  Mail,
  Phone,
  Building2,
  FileSearch,
  Gauge,
  Wand2,
} from "lucide-react";

const cn = (...c) => c.filter(Boolean).join(" ");

const Container = ({ children, className = "" }) => (
  <div className={cn("mx-auto w-full max-w-6xl px-4 sm:px-6 lg:px-8", className)}>
    {children}
  </div>
);

const Card = ({ children, className = "" }) => (
  <div
    className={cn(
      "rounded-3xl border border-slate-200 bg-white/70 p-6 shadow-sm backdrop-blur",
      className
    )}
  >
    {children}
  </div>
);

const Button = ({
  href,
  onClick,
  variant = "primary",
  className = "",
  children,
}) => {
  const base =
    "inline-flex items-center justify-center gap-2 rounded-2xl px-4 py-2.5 text-sm font-semibold transition-all focus:outline-none focus:ring-2 focus:ring-slate-400/60";
  const styles =
    variant === "primary"
      ? "bg-slate-900 text-white hover:opacity-90 shadow-sm"
      : "bg-white text-slate-900 border border-slate-200 hover:bg-slate-50";
  const Comp = href ? "a" : "button";
  return (
    <Comp href={href} onClick={onClick} className={cn(base, styles, className)}>
      {children}
    </Comp>
  );
};

const Pill = ({ children }) => (
  <span className="inline-flex items-center gap-2 rounded-full border border-slate-200 bg-white/70 px-3 py-1 text-xs font-medium text-slate-700 shadow-sm backdrop-blur">
    <Sparkles className="h-3.5 w-3.5" />
    {children}
  </span>
);

const Logo = () => (
  <div className="flex items-center gap-2">
    <div className="grid h-10 w-10 place-items-center rounded-2xl bg-slate-900 text-white shadow-sm">
      <span className="text-sm font-black tracking-tight">CF</span>
    </div>
    <div className="leading-tight">
      <div className="text-sm font-extrabold tracking-tight text-slate-900">
        ClearForm Tech
      </div>
      <div className="text-[11px] text-slate-500">AI-first product engineering</div>
    </div>
  </div>
);

const NavLink = ({ href, children }) => (
  <a href={href} className="text-sm font-medium text-slate-700 hover:text-slate-900">
    {children}
  </a>
);

const SectionTitle = ({ eyebrow, title, subtitle }) => (
  <div className="mx-auto max-w-2xl text-center">
    {eyebrow ? (
      <div className="mb-2 text-xs font-semibold uppercase tracking-wider text-slate-500">
        {eyebrow}
      </div>
    ) : null}
    <h2 className="text-2xl font-extrabold tracking-tight text-slate-900 sm:text-3xl">
      {title}
    </h2>
    {subtitle ? <p className="mt-3 text-base text-slate-600">{subtitle}</p> : null}
  </div>
);

const IconFeature = ({ icon: Icon, title, desc }) => (
  <Card className="h-full">
    <div className="flex items-start gap-4">
      <div className="grid h-10 w-10 place-items-center rounded-2xl bg-slate-900 text-white shadow-sm">
        <Icon className="h-5 w-5" />
      </div>
      <div>
        <div className="text-base font-bold text-slate-900">{title}</div>
        <p className="mt-1 text-sm text-slate-600">{desc}</p>
      </div>
    </div>
  </Card>
);

const CheckItem = ({ children }) => (
  <li className="flex gap-2 text-sm text-slate-700">
    <CheckCircle2 className="mt-0.5 h-4 w-4 flex-none" />
    <span>{children}</span>
  </li>
);

export default function ClearFormTechLanding() {
  const [navOpen, setNavOpen] = useState(false);
  const [submitted, setSubmitted] = useState(false);
  const [form, setForm] = useState({ name: "", company: "", email: "", phone: "", message: "" });

  const services = useMemo(
    () => [
      {
        icon: Workflow,
        title: "Platforms & workflow automation",
        desc: "Configurable claim intake, triage, assignment routing, QA gates, invoicing, and portals—built around your SOPs.",
      },
      {
        icon: Bot,
        title: "AI copilots for claims",
        desc: "Policy/document Q&A, voice-to-text scoping, damage tagging, estimate assist, and next-best-action prompts—with human-in-the-loop.",
      },
      {
        icon: Smartphone,
        title: "Field + desk mobile apps",
        desc: "Offline-first inspections, dynamic forms, secure media capture, and sync pipelines for high-volume teams.",
      },
      {
        icon: Database,
        title: "Integrations & data products",
        desc: "APIs, ETL, warehouses, and connectors to core systems (FNOL, CMS, estimating, payments, CRM).",
      },
      {
        icon: Lock,
        title: "Security-minded engineering",
        desc: "RBAC, least privilege, encryption, logging, and architecture aligned to enterprise controls.",
      },
      {
        icon: Gauge,
        title: "Performance at scale",
        desc: "Systems designed for peak events: queueing, caching, observability, and cost-aware infrastructure.",
      },
    ],
    []
  );

  const audiences = useMemo(
    () => [
      { icon: Building2, title: "Insurance carriers", note: "Modernize claims operations and reduce cycle time." },
      { icon: Shield, title: "Independent adjusting firms", note: "Scale consistent quality across field + desk." },
      { icon: FileSearch, title: "Public adjusting firms", note: "Streamline intake, documentation, and communication." },
      { icon: Wand2, title: "TPAs / MPAs", note: "High-volume routing, SLAs, and reporting." },
    ],
    []
  );

  const process = useMemo(
    () => [
      {
        title: "Discovery → architecture",
        desc: "We map your workflows, data, and constraints—then design a clean, modular system that can grow.",
      },
      {
        title: "Build → iterate",
        desc: "Ship weekly, measure, and refine. We prioritize reliability, speed, and excellent UX for busy claims teams.",
      },
      {
        title: "Harden → scale",
        desc: "Security controls, audit logs, observability, and performance testing—so the platform holds up under pressure.",
      },
    ],
    []
  );

  const faqs = useMemo(
    () => [
      {
        q: "Do you build web + mobile together?",
        a: "Yes. We design shared data models and APIs so your web portal and mobile app stay aligned while evolving quickly.",
      },
      {
        q: "Can you work with existing claim systems?",
        a: "Absolutely. We integrate with existing CMS/FNOL/estimating/payment tools where possible, and can modernize in phases.",
      },
      {
        q: "How do you approach AI safely?",
        a: "We use human-in-the-loop review, confidence thresholds, and auditability—so AI boosts throughput without creating hidden risk.",
      },
    ],
    []
  );

  function scrollTo(id) {
    const el = document.getElementById(id);
    if (el) el.scrollIntoView({ behavior: "smooth", block: "start" });
  }

  function onSubmit(e) {
    e.preventDefault();
    setSubmitted(true);
    // Hook this up to your backend or form provider.
    // Example: POST /api/contact with `form`
  }

  return (
    <div className="min-h-screen bg-slate-50 text-slate-900">
