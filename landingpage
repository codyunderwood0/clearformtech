                {submitted ? (
                  <div className="mt-6 rounded-3xl border border-slate-200 bg-white/70 p-6">
                    <div className="text-base font-extrabold tracking-tight">Thanks — we got it.</div>
                    <p className="mt-2 text-sm text-slate-600">
                      Replace this behavior with a real submission handler. For now, we’ve simulated a successful send.
                    </p>
                    <Button variant="secondary" className="mt-4" onClick={() => setSubmitted(false)}>
                      Send another
                    </Button>
                  </div>
                ) : (
                  <form onSubmit={onSubmit} className="mt-6 space-y-4">
                    <div className="grid gap-4 sm:grid-cols-2">
